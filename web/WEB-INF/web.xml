<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>Servlet.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>Servlet.Register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Historialpartidas</servlet-name>
        <servlet-class>Servlet.Historialpartidas</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Jugadoresonline</servlet-name>
        <servlet-class>Servlet.Jugadoresonline</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Top10</servlet-name>
        <servlet-class>Servlet.Top10</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Jugar</servlet-name>
        <servlet-class>Servlet.Jugar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Cerrarsesion</servlet-name>
        <servlet-class>Servlet.Cerrarsesion</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Historialpartidas</servlet-name>
        <url-pattern>/Historialpartidas</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Jugadoresonline</servlet-name>
        <url-pattern>/Jugadoresonline</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Top10</servlet-name>
        <url-pattern>/Top10</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Jugar</servlet-name>
        <url-pattern>/Jugar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Cerrarsesion</servlet-name>
        <url-pattern>/Cerrarsesion</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
